extends layout

block content
  h1= title
  - var image_names = ["car", "cartoon", "garden", "house", "pizza"]
  - for(let i = 0; i < 3; i++) {
    - let number = Math.floor(Math.random() * 5)
        img(src=`/images/${image_names[number]}.jpg`, alt="choosen wrong image", height="300px", width="300px")
  - }